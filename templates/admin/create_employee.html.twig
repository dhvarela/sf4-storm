{% extends 'base.html.twig' %}

{% block title %}Create employee{% endblock %}

{% block body %}

    {% include 'default/navbar.html.twig' %}

    <div class="container mt-2">
        <h1>Create employee</h1>

        {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.email) }}
        {{ form_row(form.incorporationDate) }}

        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'label.create_employee'|trans }}
        </button>

        {{ form_end(form) }}
    </div>


{% endblock %}
