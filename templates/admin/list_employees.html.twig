{% extends 'base.html.twig' %}

{% block title %}Admin actions - List employees{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/main.css') }}" />
{% endblock %}

{% block body %}

    {% include 'default/navbar.html.twig' %}


    <div class="container">
        <h1>Admin actions -> Employees</h1>
        <ul>
            {% for an_employee in employees %}
            <li><a href="{{ path('admin_edit_employee', { id: an_employee.id }) }}">{{ an_employee.name }}</a> </li>
            {% endfor %}
        </ul>
    </div>


{% endblock %}

{% block javascripts %}
{% endblock %}

