{% extends 'base.html.twig' %}

{% block title %}Create employee{% endblock %}

{% block body %}

    {% include 'default/navbar.html.twig' %}

    <div class="container mt-2">
        <h1>Create project</h1>

        {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.description) }}
        {{ form_row(form.frontend) }}
        {{ form_row(form.backend) }}

        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'label.create_work_contract'|trans }}
        </button>

        {{ form_end(form) }}
    </div>


{% endblock %}
